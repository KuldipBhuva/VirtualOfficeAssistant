@model List<EMSDomain.ViewModel.MasterItem>
@{
    ViewBag.Title = "Masters";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

  <div class="page-header">
            <h1>Master List</h1>
        </div>
<table class="table" id="tbl">
    <thead>
        <tr>
        <th>MasterType
        </th>
        <th>Desc
        </th>
        <th>Code
        </th>
        <th>
        </th>
            </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
                       
        <tr>
            <td>
                @item.MasterType
            </td>
            <td>
                @item.MasterDesc
            </td>
            <td>
                @item.MasterCode
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MId })
            </td>
        </tr>
            
    }
        </tbody>
</table>

